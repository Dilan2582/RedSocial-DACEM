# Copiar este archivo a .env y completar con tus valores reales
# NO subir .env al repositorio (est√° en .gitignore)

PORT=3900
JWT_SECRET=tu_jwt_secret_aqui

# Google OAuth
GOOGLE_CLIENT_ID=tu_google_client_id_aqui

# reCAPTCHA v2
RECAPTCHA_SITE_KEY=tu_recaptcha_site_key_aqui
RECAPTCHA_SECRET_KEY=tu_recaptcha_secret_key_aqui
RECAPTCHA_BYPASS=0

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET=tu_s3_bucket_name
AWS_ACCESS_KEY_ID=tu_aws_access_key_aqui
AWS_SECRET_ACCESS_KEY=tu_aws_secret_key_aqui

# MongoDB Atlas
MONGO_URI=tu_mongodb_uri_aqui

# Rekognition
REKOGNITION_ENABLED=true
REKOGNITION_MODE=full
